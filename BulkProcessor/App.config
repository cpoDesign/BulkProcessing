<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="akka" type="Akka.Configuration.Hocon.AkkaConfigurationSection, Akka" />
  </configSections>
          <!--debug {
            receive = on      # log any received messages
            autoreceive on    # log automatically received messages, e.g. PoisonPill
            lifecycle = on    # log actor lifecycle changes
            event-stream = on # log subscription changese for akka.net event stream
            unhandled = on    # log messages that system does not handle
          }
              <![CDATA[
        akka {
          logLevel = WARNING
          loggers = ["Akka.Logger.NLog.NLogLogger, Akka.Logger.NLog"]
          
     
      ]]>
            
              -->
  <akka>
    <hocon>
  
     <![CDATA[
          akka {            
            log-config-on-start = off            
            loglevel = WARNING

            actor {              
              debug {
                  receive = on
                  autoreceive = on
                  lifecycle = on
                  event-stream = on
                  unhandled = on
              }
              
              deployment{
                /BatchesManagerActor/BatchTypeManagerActorPayments/JobCoordinator/PaymentWorkers{
                  router = round-robin-pool
                  
                    resizer {
                      enabled = on
                      lower-bound = 3
                      upper-bound = 5                      
                    }
                }
              }
            }            
          }
      ]]>
    </hocon>
  </akka>
  <appSettings>
    <add key="BatchSize" value="15" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Collections.Immutable" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.2.1.0" newVersion="1.2.1.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>